<style>
    table,
    th,
    td {
        border: 1px solid black;
        border-collapse: collapse;
    }
</style>
<h1>Cities</h1>

<table>
    <tr>
        <th>City Code</th>
        <th>Country Code</th>
        <th>City Name</th>
        <th>Population</th>
        <th>isCoastal</th>
        <th>areaKM</th>
        <th></th>
    </tr>
    <% cities.forEach((city)=> { %>
        <tr>
            <td>
                <%= city.cty_code %>
            </td>
            <td>
                <%= city.co_code %>
            </td>
            <td>
                <%= city.cty_name %>
            </td>
            <td>
                <%= city.population %>
            </td>
            <td>
                <%= city.isCoastal %>
            </td>
            <td>
                <%= city.areaKM %>
            </td>
            <td>
                <a href='allDetails/<%= city.cty_code %>'>All Details</a>
            </td>
        </tr>
        <% }) %>
</table>

<a href='/'>Home</a>